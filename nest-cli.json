{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/all-in-one/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/al-in-one/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/all-in-one",
  "projects": {
    "third-party-server": {
      "type": "application",
      "root": "apps/third-party-server",
      "entryFile": "main",
      "sourceRoot": "apps/third-party-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/third-party-server/tsconfig.app.json"
      }
    },
    "all-in-one": {
      "type": "application",
      "root": "apps/all-in-one",
      "entryFile": "main",
      "sourceRoot": "apps/all-in-one/src",
      "compilerOptions": {
        "tsConfigPath": "apps/all-in-one/tsconfig.app.json"
      }
    }
  }
}